{
  "name": "kazbegi",
  "version": "1.0.0",
  "description": "Pure Html, Saas Project.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/style.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass scss/style.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icons-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koexygen/kazbegi.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/koexygen/kazbegi/issues"
  },
  "homepage": "https://github.com/koexygen/kazbegi#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0"
  },
  "dependencies": {
    "live-server": "^1.2.1"
  }
}
